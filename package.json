{
  "name": "frozen-bo",
  "version": "0.0.1",
  "main": "src",
  "private": true,
  "scripts": {
    "start": "node .",
    "slow1s": "cross-env SLOW_MODE=1000 node .",
    "slow5s": "cross-env SLOW_MODE=5000 node .",
    "slow10s": "cross-env SLOW_MODE=10000 node .",
    "slow60s": "cross-env SLOW_MODE=60000 node .",
    "test": "jest",
    "test:item": "jest src/api/item/index.test.js",
    "test:user": "jest src/api/user/index.test.js",
    "coverage": "npm test -- --coverage",
    "postcoverage": "open-cli coverage/lcov-report/index.html",
    "debug": "nodemon --inspect .",
    "dev": "nodemon -i \"*.test.js\" .",
    "prod": "cross-env NODE_ENV=production nodemon -i \"*.test.js\" -r dotenv-safe/config .",
    "lint": "eslint src",
    "docs": "apidoc -i src -o docs && apidoc-markdown -p docs -o DOCS.md",
    "postdocs": "open-cli docs/index.html"
  },
  "jest": {
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "<rootDir>/generators/",
      "<rootDir>/node_modules/"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/test/setup.js"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/plugin-proposal-export-default-from": "^7.10.4",
    "@babel/plugin-transform-runtime": "^7.11.5",
    "@babel/preset-env": "^7.11.5",
    "@rigwild/apidoc-markdown": "^3.2.2",
    "apidoc": "^0.25.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.3.0",
    "cross-env": "^7.0.2",
    "dotenv-safe": "^8.2.0",
    "eslint": "^7.10.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "jest-cli": "^26.4.2",
    "mongodb-memory-server": "^6.8.0",
    "nock": "^13.0.4",
    "nodemon": "^2.0.4",
    "open-cli": "^6.0.1",
    "sinon": "^9.0.3",
    "supertest": "^5.0.0"
  },
  "dependencies": {
    "@babel/register": "^7.11.5",
    "@babel/runtime": "^7.11.2",
    "@sendgrid/mail": "^7.2.6",
    "bcrypt": "^5.0.0",
    "bluebird": "^3.7.2",
    "body-parser": "^1.19.0",
    "bodymen": "^1.1.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "jimp": "^0.16.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.10.7",
    "mongoose-create-unique": "^0.4.4",
    "mongoose-keywords": "^0.4.1",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "passport": "^0.4.1",
    "passport-http": "^0.3.0",
    "passport-http-bearer": "^1.0.1",
    "passport-jwt": "^4.0.0",
    "querymen": "^2.1.4",
    "rand-token": "^1.0.1",
    "request": "^2.88.2",
    "request-promise": "^4.2.5",
    "shortid": "^2.2.15"
  }
}
