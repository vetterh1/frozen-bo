{
  "name": "frozen-bo",
  "version": "0.0.1",
  "main": "src",
  "private": true,
  "scripts": {
    "start": "node .",
    "test": "jest",
    "test:items": "jest src/api/item/index.test.js",
    "coverage": "npm test -- --coverage",
    "postcoverage": "open-cli coverage/lcov-report/index.html",
    "dev": "nodemon -i \"*.test.js\" .",
    "prod": "cross-env NODE_ENV=production nodemon -i \"*.test.js\" -r dotenv-safe/config .",
    "lint": "eslint src",
    "docs": "apidoc -i src -o docs && apidoc-markdown -p docs -o DOCS.md",
    "postdocs": "open-cli docs/index.html"
  },
  "jest": {
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "<rootDir>/generators/",
      "<rootDir>/node_modules/"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/test/setup.js"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@babel/plugin-proposal-export-default-from": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "@rigwild/apidoc-markdown": "^2.0.2",
    "apidoc": "^0.17.6",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.8.0",
    "cross-env": "^5.0.5",
    "dotenv-safe": "^6.1.0",
    "eslint": "^5.16.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-node": "^9.0.1",
    "eslint-plugin-promise": "^4.1.1",
    "eslint-plugin-standard": "^4.0.0",
    "jest-cli": "^24.8.0",
    "mongodb-memory-server": "^5.1.1",
    "nock": "^10.0.6",
    "nodemon": "^1.10.2",
    "open-cli": "^5.0.0",
    "sinon": "^7.3.2",
    "supertest": "^4.0.2"
  },
  "dependencies": {
    "@babel/register": "^7.4.4",
    "@babel/runtime": "^7.4.4",
    "@sendgrid/mail": "^6.3.1",
    "bcrypt": "^3.0.6",
    "bluebird": "^3.5.1",
    "body-parser": "^1.18.2",
    "bodymen": "^1.0.3",
    "compression": "^1.7.1",
    "cors": "^2.8.4",
    "express": "^4.16.2",
    "jimp": "^0.6.4",
    "jsonwebtoken": "^8.1.0",
    "mongoose": "^5.1.0",
    "mongoose-create-unique": "^0.4.4",
    "mongoose-keywords": "^0.4.0",
    "morgan": "^1.7.0",
    "multer": "^1.4.2",
    "passport": "^0.4.0",
    "passport-http": "^0.3.0",
    "passport-http-bearer": "^1.0.1",
    "passport-jwt": "^4.0.0",
    "querymen": "^2.1.3",
    "rand-token": "^0.4.0",
    "request": "^2.83.0",
    "request-promise": "^4.2.2",
    "shortid": "^2.2.14"
  }
}
